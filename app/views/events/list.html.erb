	<script type="text/javascript">
		function paginationIncrementCalendar(id){
			$('#mes' + id ).hide(); 
			$('#mes' + (id+1)).show();
		}

		function paginationDecrementCalendar(id){
			$('#mes' + id ).hide(); 
			$('#mes' + (id-1)).show();
		}

		var events_number = 5;
		function showDetail(idEvent){
			for(i = 1; i <= events_number; i++){
				if(i == idEvent){
					$('#date-'+i+'-detail').show();
				} else {
					$('#date-'+i+'-detail').hide();
				}
			}
		}

